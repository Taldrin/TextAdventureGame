<Project Sdk="Tizen.NET.Sdk/1.1.6">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>tizen50</TargetFramework>
    <OutputType>Exe</OutputType>
    <DefineConstants>$(DefineConstants);__TIZEN__;</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="lib\" />
    <Folder Include="res\" />
  </ItemGroup>
  <ItemGroup Condition="exists('..\Furventure.AdventureGames.Offline.UWP')">
    <EmbeddedResource Include="..\Furventure.AdventureGames.Offline.UWP\Package.appxmanifest" LogicalName="Package.appxmanifest" />
    <Content Include="..\Furventure.AdventureGames.Offline.UWP\Assets\StoreLogo.png" Link="Assets\StoreLogo.png" />
  </ItemGroup>
  <ItemGroup>
    <UpToDateCheckInput Include="..\Furventure.AdventureGames.Offline.Shared\**\*.xaml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="5.0.0" />
    <PackageReference Include="SkiaSharp.Views" Version="2.80.2" />
    <PackageReference Include="Uno.WinUI.Skia.Tizen" Version="3.9.1" />
    <PackageReference Include="Uno.WinUI.RemoteControl" Version="3.9.1" Condition="'$(Configuration)'=='Debug'" />
  </ItemGroup>
  <Import Project="..\Furventure.AdventureGames.Offline.Shared\Furventure.AdventureGames.Offline.Shared.projitems" Label="Shared" />
</Project>